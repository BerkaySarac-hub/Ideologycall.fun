<h1>All Users</h1>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Nickname</th>
            <th>Email</th>
            <th>Ideology</th>
            <th>Profile Picture</th>
        </tr>
    </thead>
    <tbody>
        <% for(var i=0; i<users.length; i++) { %>
            <tr>
                <td>
                    <%= users[i].id %>
                </td>
                <td>
                    <%= users[i].Nickname %>
                </td>
                <td>
                    <%= users[i].Email %>
                </td>
                <td>
                    <%= users[i].Ideology %>
                </td>
                <td><img src="<%=users[i].ProfilePicture %>" width="100"></td>
            </tr>
            <% } %>
    </tbody>
</table>